{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "BloggieDbConnectionString": "Server=localhost;Database=BloggieDb;Trusted_Connection=True;TrustServerCertificate=True"
  },
  //Replace by your Cloudinary info
  "Cloudinary": {
    "CloudName": "%CLOUDINARY_CLOUD_NAME%", 
    "ApiKey": "%CLOUDINARY_API_KEY%",
    "ApiSecret": "%CLOUDINARY_API_SECRET%"
  }
}
